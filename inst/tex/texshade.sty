       \total@frequency@correction
     \fi
    \fi
    \loopcount=1
    \loop
          \xdef\first@{\csname @rd\the\loopcount\endcsname} %%%
          \expandafter\xdef\csname seq\the\loopcount\endcsname{\csname sequence\first@\endcsname &@}
       \advance\loopcount by 1
