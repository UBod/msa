all: muscle

muscle:
	export PKG_CXXFLAGS="$(PKG_CXXFLAGS) -I../gc-7.2/include"
	${R_HOME}/bin${R_ARCH_BIN}/R CMD SHLIB -o libMuscle.so $(CPPNames)
	ar rcs libMuscle.a $(OBJNames)
	cp libMuscle.a ../
